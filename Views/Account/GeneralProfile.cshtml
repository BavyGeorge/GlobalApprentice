@model Global_Intern.Models.User
@{
    ViewData["Title"] = "CompleteProfile";
    Layout = "~/Views/Shared/_LayoutDashBoard.cshtml";
}
@*
     UserFirstName { get; set; }
     UserLastName { get; set; }
     UserEmail { get; set; }
     UserAddress { get; set; } //CP
     UserCity { get; set; } //CP
     UserState { get; set; } //CP
     UserCountry { get; set; } //CP
     UserZip { get; set; } //CP
     UserPhone { get; set; }
     UserImage { get; set; }
     UserGender { get; set; } // Could be use full for User with student role.
    Role Role { get; set; } //FK*@

<div class="wrapper d-flex">
    <div class="sideMenu">
        <div class="sidebar">
            <ul class="navbar-nav">
                <li class="nav-item">
                    <a href="#" class="nav-link sidebarLinks">
                        <i class="material-icons icon">dashboard</i>
                        <span class="text">Dashboard</span>
                    </a>
                </li>
                <li class="nav-item">
                    <a href="#" class="nav-link sidebarLinks">
                        <i class="material-icons icon">person</i>
                        <span class="text">Edit Profile</span>
                    </a>
                </li>
                <li class="nav-item">
                    <a href="#" class="nav-link sidebarLinks">
                        <i class="material-icons icon">insert_chart</i>
                        <span class="text">AllInternships</span>
                    </a>
                </li>
                <li class="nav-item">
                    <a href="#" class="nav-link sidebarLinks">
                        <i class="material-icons icon">settings</i>
                        <span class="text">Settings</span>
                    </a>
                </li>
                @*<li class="nav-item">
                        <a href="#" class="nav-link sidebarLinks">
                            <i class="material-icons icon">view_list</i>
                            <span class="text">Resize</span>
                        </a>
                    </li>*@
            </ul>
        </div>
    </div>
    <div class="content">
        <main>
            <div class="container-fluid" style="margin-top:56px;">
                <form id="EmployerProfile" asp-action="CompleteProfile" enctype="multipart/form-data" method="post">
                    <div asp-validation-summary="ModelOnly" class="text-danger"></div>
                    <div class="input-group mb-3">
                        <div class="input-group-prepend">
                            <span class="input-group-text">Upload</span>
                        </div>
                        <div class="custom-file">
                            <input type="file" asp-for="UserImage">
                            <label class="custom-file-label" for="UserImage">Choose file</label>
                        </div>
                    </div>
                    <div>
                        <label for="UserFirstName">UserFirstNameo</label>
                        <input class="form-control" asp-for="UserFirstName">
                    </div>
                    <div class="form-group">
                        <label for="UserLastName">UserLastName</label>
                        <input class="form-control" asp-for="UserLastName">
                    </div>
                    <div class="form-group">
                        <label for="UserGender">UserLastName</label>
                        <input class="form-control" asp-for="UserGender">
                    </div>
                    <div class="form-group">
                        <label for="UserAddress">UserAddress</label>
                        <input class="form-control" asp-for="UserAddress">
                    </div>
                    <div class="form-group">
                        <label for="UserCity">UserCity</label>
                        <input class="form-control" asp-for="UserCity">
                    </div>
                    <div class="form-group">
                        <label for="UserState">UserState</label>
                        <input class="form-control" asp-for="UserState">
                    </div>
                    <div class="form-group">
                        <label for="UserCountry">UserCountry</label>
                        <input class="form-control" asp-for="UserCountry">
                    </div>
                    <div class="form-group">
                        <label for="UserZip">UserZip</label>
                        <input class="form-control" asp-for="UserZip">
                    </div>
                    <div class="form-group">
                        <label for="UserPhone">UserPhone</label>
                        <input class="form-control" type="number" asp-for="UserPhone">
                    </div>
                    @*<button class="btn-back" asp-area="" asp-controller="Home" asp-action="Index">Back to Home</button>*@
                    <input type="submit" class="btn btn-info" value="Save">
                </form>
            </div>
        </main>
    </div>
</div>


<!-- Optional JavaScript -->
<!-- jQuery first, then Popper.js, then Bootstrap JS -->
<script src="https://code.jquery.com/jquery-3.5.1.js" integrity="sha256-QWo7LDvxbWT2tbbQ97B53yJnYU3WhH/C8ycbRAkjPDc=" crossorigin="anonymous"></script>
<script src="https://cdnjs.cloudflare.com/ajax/libs/popper.js/1.14.7/umd/popper.min.js" integrity="sha384-UO2eT0CpHqdSJQ6hJty5KVphtPhzWj9WO1clHTMGa3JDZwrnQq4sF86dIHNDz0W1" crossorigin="anonymous"></script>
<script src="https://stackpath.bootstrapcdn.com/bootstrap/4.3.1/js/bootstrap.min.js" integrity="sha384-JjSmVgyd0p3pXB1rRibZUAYoIIy6OrQ6VrjIEaFf/nJGzIxFDsf4x0xIM+B07jRM" crossorigin="anonymous"></script>
<script src="https://cdnjs.cloudflare.com/ajax/libs/jQuery-slimScroll/1.3.8/jquery.slimscroll.js" integrity="sha512-37SbZHAnGzLuZV850k61DfQdZ5cnahfloYHizjpEwDgZGw49+D6oswdI8EX3ogzKelDLjckhvlK0QZsY/7oxYg==" crossorigin="anonymous"></script>
<script type="text/javascript" charset="utf-8">
    $(document).ready(function () {

        $('.sideMenuToggler').on('click', function () {
            console.log("clicked");
            $('.wrapper').toggleClass('active');
        });

        var adjustSidebar = function () {
            $('.sidebar').slimScroll({
                height: document.documentElement.clientHeight - $('.navbar').outerHeight()
            });
        };

        adjustSidebar();
        $(window).resize(function () {
            adjustSidebar();
        });
    });
</script>